# Ultravox-Twilio Integration Service Configuration
# Copy this file to .env and fill in your actual values

# Ultravox API Configuration
ULTRAVOX_API_KEY=your_ultravox_api_key_here
ULTRAVOX_BASE_URL=https://api.ultravox.ai

# Twilio API Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=+1234567890
TWILIO_USER_SID=your_twilio_user_sid_here

# Application Configuration
DEBUG=false
LOG_LEVEL=INFO
PORT=8000

# Security
API_KEY=your_secure_api_key_here

# ElevenLabs Configuration (Optional)
ENABLE_ELEVENLABS=false
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_BASE_URL=https://api.elevenlabs.io
ELEVENLABS_DEFAULT_VOICE=21m00Tcm4TlvDq8ikWAM
ELEVENLABS_MAX_TEXT_LENGTH=5000
ELEVENLABS_REQUEST_TIMEOUT=30.0
ELEVENLABS_PREVIEW_ENABLED=true

# Optional: Database Configuration (if using persistent storage)
# DATABASE_URL=sqlite:///./app.db